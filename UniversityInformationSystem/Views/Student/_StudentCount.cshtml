@model  List<UniversityInformationSystem.Models.StudentCountModel>

<table class="table" style="width: 70%; margin-left: auto; margin-right: auto;">
    <thead class="thead-dark">
        <tr>
            <th scope="col">Dersin Adı</th>
            <th scope="col">Öğrenci Sayısı</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.DersAdi</td>
                <td>@item.OgrenciSayisi</td>
            </tr>
        }
    </tbody>
</table>